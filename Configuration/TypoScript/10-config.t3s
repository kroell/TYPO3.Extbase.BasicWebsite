config {
	# locallang.xlf
	language = de
	# PHP: Zeit/WÃ¤hrung/...
	locale_all = de_DE.utf8
	# DB
	sys_language_uid = 0
	# Nur Inhalte als Fallback in der Std. Sprache
	sys_language_mode = content_fallback
	# GET-Parameter mit Validierung
	linkVars = L(1-2)

	sendCacheHeaders = 1
	cache_period = 86400

	disablePrefixComment = 1

	disableImgBorderAttr = 1
}
